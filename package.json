    "next-env": "1.1.1",
    "node-fetch": "3.1.0",
    "node-gyp": "^8.4.1",
    "parcel-bundler": "1.12.5",
    "pg": "8.7.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "1.44.0",
    "shopify": "0.1.0",
