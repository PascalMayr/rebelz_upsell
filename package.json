{
  "name": "salestorm",
  "version": "1.0.1",
  "description": "A Shopify app to boost sales.",
  "scripts": {
    "graphile": "cd server && DOTENV_CONFIG_PATH=../.env node -r dotenv/config ../node_modules/graphile-migrate/dist/cli.js",
    "dev": "yarn run graphile migrate && cross-env NODE_ENV=development PORT=443 nodemon --inspect ./server/index.js --watch ./server/index.js & parcel watch scripts/startup.js -d public",
    "serve": "shopify app serve --host https://loop.rebelzcommerce.com",
    "build": "NEXT_TELEMETRY_DISABLED=1 next build && parcel build scripts/startup.js --no-source-maps",
    "release": "yarn run graphile migrate && node scripts/startup-to-github",
    "start": "cross-env NODE_ENV=production node ./server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PascalMayr/salestorm_upsell.git"
  },
  "author": "Pascal Mayr, Sascha Mayr",
  "engines": {
    "node": "14.x"
  },
  "browserslist": [
    "since 2017-06"
  ],
  "cacheDirectories": [
    ".next/cache"
  ],
  "dependencies": {
    "@apollo/client": "3.5.6",
    "@babel/core": "7.16.0",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-react": "7.16.0",
    "@babel/register": "7.16.0",
    "@koa/cors": "3.1.0",
    "@sendgrid/mail": "7.6.0",
    "@sentry/browser": "6.16.0",
    "@sentry/node": "6.16.0",
    "@sentry/react": "6.16.0",
    "@sentry/tracing": "6.16.0",
    "@shopify/app-bridge": "2.0.5",
    "@shopify/app-bridge-react": "2.0.5",
    "@shopify/app-bridge-utils": "2.0.5",
    "@shopify/app-cli-node-generator-helper": "1.2.2",
    "@shopify/koa-shopify-auth": "4.1.4",
    "@shopify/koa-shopify-webhooks": "2.6.4",
    "@shopify/polaris": "6.6.0",
    "@shopify/polaris-icons": "4.10.0",
    "axios": "0.21.4",
    "casify": "2.0.0",
